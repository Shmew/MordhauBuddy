{
  "name": "mordhau-buddy",
  "version": "0.0.1",
  "description": "Compilation of Mordhau Tools",
  "homepage": "https://github.com/Shmew/MordhauBuddy",
  "bugs": {
    "url": "https://github.com/Shmew/MordhauBuddy/issues"
  },
  "license": "MIT",
  "author": "Shmew",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shmew/MordhauBuddy.git"
  },
  "scripts": {
    "compile": "electron-webpack",
    "dev": "electron-webpack dev",
    "dist:dirLinux": "yarn distLinux --dir -c.compression=store -c.mac.identity=null",
    "dist:dirWin": "yarn distWin --dir -c.compression=store -c.mac.identity=null",
    "dist:dirWin64": "yarn distWin64 --dir -c.compression=store -c.mac.identity=null",
    "distLinux": "yarn compile && electron-builder --linux --x64",
    "distWin": "yarn compile && electron-builder --win --ia32",
    "distWin64": "yarn compile && electron-builder --win --x64",
    "publishLinux": "yarn distLinux && electron-builder --publish always",
    "publishWin": "yarn distWin && electron-builder --publish always",
    "publishWin64": "yarn distWin64 && electron-builder --publish always"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/Electron/Main",
      "webpackConfig": "webpack.additions.main.js"
    },
    "renderer": {
      "sourceDirectory": "src/Electron/Renderer",
      "webpackConfig": "webpack.additions.renderer.js"
    },
    "title": true
  },
  "build": {
    "appId": "com.electron.mordhau-buddy",
    "directories": {
      "output": "dist/${os}-${arch}"
    },
    "extraFiles": [
      "static"
    ],
    "extraResources": {
      "filter": [
        "**/*",
        "!**/*.pdb",
        "!**/*.xml"
      ],
      "from": "./Bin/Core/netcoreapp3.0/${os}-${arch}",
      "to": "extraResources"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "productName": "MordhauBuddy",
    "publish": [
      {
        "provider": "github",
        "vPrefixedTagName": false
      }
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.23",
    "autosuggest-highlight": "^3.1.1",
    "downshift": "^3.2.10",
    "electron-dl": "^1.14.0",
    "electron-store": "^4.0.0",
    "electron-updater": "^4.1.2",
    "match-sorter": "^4.0.1",
    "mdi-material-ui": "6.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "source-map-support": "^0.5.12",
    "toastr": "^2.1.4",
    "typeface-roboto": "^0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "electron": "6.0.0-beta.13",
    "electron-builder": "^21.1.5",
    "electron-devtools-installer": "^2.2.4",
    "electron-webpack": "^2.7.4",
    "electron-window-state": "^5.0.3",
    "fable-compiler": "^2.3.14",
    "fable-loader": "^2.1.8",
    "node-sass": "^4.12.0",
    "remotedev": "^0.2.9",
    "sass-loader": "^7.1",
    "webpack": "^4.38.0"
  }
}