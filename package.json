{
  "name": "mordhau-buddy",
  "description": "Compilation of Mordhau Tools",
  "version": "0.0.1",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shmew/MordhauBuddy.git"
  },
  "author": "Shmew",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shmew/MordhauBuddy/issues"
  },
  "homepage": "https://github.com/Shmew/MordhauBuddy",
  "electronWebpack": {
    "title": true,
    "main": {
      "sourceDirectory": "src/Electron/Main",
      "webpackConfig": "webpack.additions.main.js"
    },
    "renderer": {
      "sourceDirectory": "src/Electron/Renderer",
      "webpackConfig": "webpack.additions.renderer.js"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "autosuggest-highlight": "^3.1.1",
    "downshift": "^3.2.10",
    "electron-store": "^4.0.0",
    "match-sorter": "^4.0.1",
    "mdi-material-ui": "6.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "source-map-support": "^0.5.12",
    "toastr": "^2.1.4",
    "typeface-roboto": "^0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "electron": "6.0.0-beta.13",
    "electron-builder": "^21.1.5",
    "electron-devtools-installer": "^2.2.4",
    "electron-webpack": "^2.7.4",
    "electron-window-state": "^5.0.3",
    "fable-compiler": "^2.3.14",
    "fable-loader": "^2.1.8",
    "node-sass": "^4.12.0",
    "remotedev": "^0.2.9",
    "sass-loader": "^7.1",
    "webpack": "^4.38.0"
  }
}